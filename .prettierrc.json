{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "singleAttributePerLine": true,
  "bracketSameLine": true,
  "htmlWhitespaceSensitivity": "ignore",
  "plugins": ["prettier-plugin-organize-attributes"],
  "attributeGroups": [
    "$ANGULAR_STRUCTURAL_DIRECTIVE",
    "^id$",
    "^name$",
    "^class$",
    "^aria-",
    "$DEFAULT",
    "$ANGULAR_INPUT",
    "$ANGULAR_TWO_WAY_BINDING",
    "$ANGULAR_OUTPUT"
  ]
}
